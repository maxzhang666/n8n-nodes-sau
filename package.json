{
  "name": "@maxzhang666/n8n-nodes-sau",
  "version": "1.0.9",
  "description": "n8n node for Social media Auto Upload",
  "license": "MIT",
  "keywords": [
    "n8n-community-node-package",
    "小红书",
    "xiaohongshu",
    "red book",
    "抖音",
    "douyin",
    "tiktok"
  ],
  "author": {
    "name": "maxzhang666"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxzhang666/n8n-nodes-sau"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "n8n-node build",
    "build:glup": "tsc && gulp build:icons",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/PlatformApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Xiaohongshu/Xiaohongshu.node.js",
      "dist/nodes/Douyin/Douyin.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "@n8n/node-cli": "*",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "gulp": "^4.0.2",
    "typescript": "5.9.2"
  }
}